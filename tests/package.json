{
  "name": "gugut-ui-tests",
  "version": "1.0.0",
  "description": "UI tests for Gugut landing page using Selenium WebDriver",
  "main": "index.js",
  "scripts": {
    "test": "node run-tests.js",
    "test:single": "mocha --timeout 30000",
    "report": "mocha tests/**/*.test.js --timeout 30000 --reporter mochawesome",
